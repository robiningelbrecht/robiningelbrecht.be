<section id="blog" class="py-24">
    <div class="container">
        <h3>Latest blog posts.</h3>
        <div class="grid sm:grid-cols-2 md:grid-cols-3 xl:grid-cols-4 gap-7">
            {% for blogPost in blogPosts %}
                <div class="rounded-md shadow-md">
                    <div class="rounded-t-md overflow-hidden">
                        <img class="w-full h-[170px] object-cover transition-all duration-500 hover:scale-125"
                             src="{{ blogPost.getImage() }}" alt="{{ blogPost.getTitle()|raw }}"/>
                    </div>
                    <div class="my-5 mx-4">
                        <div class="mb-2.5 text-sm font-semibold text-light-grey">
                            {{ blogPost.getPubDate().format('D M d Y, H:i') }}
                        </div>
                        <h4 class="mb-3 h-[67px] overflow-hidden">
                            <a href="{{ blogPost.getLink() }}">{{ blogPost.getTitle()|raw }}</a>
                        </h4>
                        <span class="text-base border-b-2 border-primary">
                            <a href="{{ blogPost.getLink() }}">Read More</a>
                        </span>
                    </div>
                </div>
            {% endfor %}
        </div>
    </div>
</section>
