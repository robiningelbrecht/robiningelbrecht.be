<script src="https://unpkg.com/typed.js@2.1.0/dist/typed.umd.js"></script>
<script>
    window.addEventListener('load', function (event) {
        new Typed('.typed', {
            stringsElement: '.typed-strings',
            loop: true,
            typeSpeed: 20,
            backSpeed: 20,
            backDelay: 2000,
        });

        const navbar = document.querySelector("nav");
        // Add scroll event listener
        window.addEventListener("scroll", function () {
            const bodyScroll = window.scrollY;
            if (bodyScroll > 96) {
                navbar.classList.add("nav-scroll");
            } else {
                navbar.classList.remove("nav-scroll");
            }
        });

        // Lazy load high def version of background.
        const img = new Image();
        const $elWithBackground = document.querySelector('[data-background]');
        img.onload = () => {
            $elWithBackground.style.backgroundImage = 'url(' + $elWithBackground.dataset.background + ')';
        };
        img.src = $elWithBackground.dataset.background;
    });
</script>

